{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    {
      "identifier": "fs:scope-config-recursive"
    },
    {
      "identifier": "fs:scope-document-recursive"
    },
    {
      "identifier": "fs:scope-download-recursive"
    },
    {
      "identifier": "fs:create-app-specific-dirs"
    },
    {
      "identifier": "fs:read-app-specific-dirs-recursive"
    },
    {
      "identifier": "core:path:default"
    },
    {
      "identifier": "core:path:allow-resolve-directory"
    }
  ]
}
